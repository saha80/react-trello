{
  "parser": "@babel/eslint-parser",
  "rules": {
    "strict": 1,
    "no-undef": 1,
    "no-fallthrough": 1,
    "react/prop-types": 2
  },
  "plugins": ["react"],
  "extends": [
    "standard",
    "standard-react",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  }
}
