{
  "name": "react-trello",
  "version": "2.4.1",
  "description": "Pluggable components to add a trello like kanban board to your application",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9002",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "istanbul cover -x *.test.js _mocha -- -R spec --require tests/config/setup 'tests/**/*.test.js'",
    "test:smoke": "start-storybook --smoke-test",
    "lint:prettier": "prettier --write ./**/* --ignore-path ./.gitignore",
    "lint:eslint": "eslint . --ignore-path ./.gitignore --fix",
    "lint:tsc": "tsc --noEmit",
    "build": "babel src --out-dir dist --copy-files",
    "docs": "build-storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rcdexta/react-trello"
  },
  "keywords": [
    "react",
    "trello",
    "board"
  ],
  "author": "RC, Prakash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcdexta/react-trello/issues"
  },
  "homepage": "https://github.com/rcdexta/react-trello",
  "dependencies": {
    "autosize": "^4.0.2",
    "classnames": "^2.2.6",
    "immutability-helper": "^2.8.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react-popopo": "^2.1.9",
    "react-redux": "^7.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-logger": "^3.0.6",
    "trello-smooth-dnd": "1.0.0",
    "uuid": "^3.3.2"
